@{
    var session = (startup_website_asp.net.Common.AdminLogin)Session[startup_website_asp.net.Common.CommonSession.ADMIN_SESSION];
}
<!doctype html>
<html class="no-js" lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>STARTUP | @ViewBag.Title</title>
    <meta name="description" content="">
    <meta name="keywords" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://fonts.googleapis.com/css?family=Nunito+Sans:300,400,600,700,800" rel="stylesheet">
    <!-- Favicon -->
	<link rel="icon" type="image/png" href="/Content/images/favicon.png">
    <link rel="stylesheet" href="~/Areas/Admin/Assets/plugins/bootstrap/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Areas/Admin/Assets/plugins/fontawesome-free/css/all.min.css">
    <link rel="stylesheet" href="~/Areas/Admin/Assets/plugins/icon-kit/dist/css/iconkit.min.css">
    <link rel="stylesheet" href="~/Areas/Admin/Assets/plugins/ionicons/dist/css/ionicons.min.css">
    <link rel="stylesheet" href="~/Areas/Admin/Assets/plugins/perfect-scrollbar/css/perfect-scrollbar.css">
    <link rel="stylesheet" href="~/Areas/Admin/Assets/plugins/datatables.net-bs4/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" href="~/Areas/Admin/Assets/plugins/jvectormap/jquery-jvectormap.css">
    <link rel="stylesheet" href="~/Areas/Admin/Assets/plugins/tempusdominus-bootstrap-4/build/css/tempusdominus-bootstrap-4.min.css">
    <link rel="stylesheet" href="~/Areas/Admin/Assets/plugins/weather-icons/css/weather-icons.min.css">
    <link rel="stylesheet" href="~/Areas/Admin/Assets/plugins/c3/c3.min.css">
    <link rel="stylesheet" href="~/Areas/Admin/Assets/plugins/owl.carousel/dist/assets/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Areas/Admin/Assets/plugins/owl.carousel/dist/assets/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/Areas/Admin/Assets/plugins/summernote/dist/summernote-bs4.css">
    <link rel="stylesheet" href="~/Areas/Admin/Assets/plugins/jquery-toast-plugin/dist/jquery.toast.min.css">
    <link rel="stylesheet" href="~/Areas/Admin/Assets/dist/css/theme.min.css">
    @*Style for each page*@
    @RenderSection("style", required: false)
    <script src="~/Areas/Admin/Assets/src/js/vendor/modernizr-2.8.3.min.js"></script>


    <script src="~/Areas/Admin/Assets/src/js/vendor/jquery-3.3.1.min.js"></script>
</head>
<body>
    <div class="wrapper">
        @RenderPage("_TopNavbar.cshtml")
        <div class="page-wrap">
            @RenderPage("_LeftNavbar.cshtml")
            <div class="main-content">
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </div>
            @RenderPage("_Footer.cshtml")
        </div>
    </div>

    <script src="~/Areas/Admin/Assets/plugins/popper.js/dist/umd/popper.min.js"></script>
    <script src="~/Areas/Admin/Assets/plugins/bootstrap/dist/js/bootstrap.min.js"></script>
    <script src="~/Areas/Admin/Assets/plugins/perfect-scrollbar/dist/perfect-scrollbar.min.js"></script>
    <script src="~/Areas/Admin/Assets/plugins/screenfull/dist/screenfull.js"></script>
    <script src="~/Areas/Admin/Assets/plugins/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/Areas/Admin/Assets/plugins/datatables.net-bs4/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/Areas/Admin/Assets/plugins/datatables.net-responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/Areas/Admin/Assets/plugins/datatables.net-responsive-bs4/js/responsive.bootstrap4.min.js"></script>
    <script src="~/Areas/Admin/Assets/plugins/jvectormap/jquery-jvectormap.min.js"></script>
    <script src="~/Areas/Admin/Assets/plugins/moment/moment.js"></script>
