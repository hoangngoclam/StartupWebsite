    <script src="~/Areas/Admin/Assets/plugins/d3/dist/d3.min.js"></script>
    <script src="~/Areas/Admin/Assets/plugins/c3/c3.min.js"></script>
    <script src="~/Areas/Admin/Assets/plugins/summernote/dist/summernote-bs4.min.js"></script>
    <script src="~/Areas/Admin/Assets/plugins/sweetalert/dist/sweetalert.min.js"></script>

    <script src="~/Areas/Admin/Assets/js/widgets.js"></script>
    <script src="~/Areas/Admin/Assets/js/charts.js"></script>
    <script src="~/Areas/Admin/Assets/dist/js/theme.min.js"></script>

    <script src="~/Content/js/Common/actionTable.js"></script>
    @*<script src="~/Areas/Admin/Assets/js/tables.js"></script>*@
    @RenderSection("script", required: false)
</body>
</html>
